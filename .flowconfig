[ignore]
.*/example/.*
.*/test/fixtures/.*

[include]

[libs]
flow-typed/
packages/tessellate-bundler/flow-typed
packages/tessellate-editor/flow-typed
packages/tessellate-fragment/flow-typed
packages/tessellate-render/flow-typed
packages/tessellate-request/flow-typed
packages/tessellate-server/flow-typed
packages/tessellate-transform/flow-typed
packages/tessellate-viewer/flow-typed

[options]
all=false
emoji=true
module.file_ext=.js

esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
experimental.strict_type_args=true

suppress_comment=\\(.\\|\n\\)*\\$FlowIgnore
