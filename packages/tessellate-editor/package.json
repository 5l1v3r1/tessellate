{
  "name": "tessellate-editor",
  "version": "0.1.0-alpha.2",
  "description": "Tessellate editor service.",
  "author": "Maximilian Fellner <maximilian.fellner@zalando.de>",
  "license": "MIT",
  "main": "dist/server.js",
  "publishConfig": {
    "tag": "next"
  },
  "bin": {
    "tessellate-editor": "./bin/tessellate-editor"
  },
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zalando-incubator/tessellate.git"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "dist-client": "NODE_ENV=production webpack --config webpack.client.js -p",
    "dist-server": "babel -d dist/server server",
    "dist": "concurrently \"npm run dist-client\" \"npm run dist-server\"",
    "start-client": "webpack-dev-server --inline --config webpack.client.js --content-base dist/client",
    "start-server": "DEBUG=tessellate-editor:* STATIC_DIR=dist/client babel-node server/server.js",
    "start": "concurrently \"npm run start-client\" \"npm run start-server\"",
    "lint": "eslint .",
    "test": "NODE_ENV=test jest --coverage",
    "flow-check": "flow check",
    "flow-typed-install": "rimraf flow-typed/npm/*_vx.x.x.js && flow-typed install -o",
    "prepublishOnly": "npm run clean && npm run dist"
  },
  "dependencies": {
    "debug": "2.2.0",
    "isomorphic-fetch": "2.2.1",
    "js-yaml": "3.6.1",
    "koa": "2.0.0-alpha.7",
    "koa-morgan": "1.0.1",
    "koa-router": "7.0.1",
    "koa-static": "3.0.0",
    "mz": "2.4.0",
    "nconf": "0.8.4",
    "request": "2.75.0",
    "request-promise-native": "1.0.3"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-syntax-flow": "6.18.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-latest-minimal": "1.1.2",
    "babel-preset-react": "6.22.0",
    "concurrently": "3.3.0",
    "eslint": "3.7.1",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-flowtype": "2.19.0",
    "eslint-plugin-promise": "2.0.1",
    "eslint-plugin-react": "6.3.0",
    "flow-bin": "0.39.0",
    "flow-typed": "2.0.0",
    "html-webpack-plugin": "2.28.0",
    "jest": "18.1.0",
    "raw-loader": "0.5.1",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "rimraf": "2.5.4",
    "supertest": "2.0.0",
    "supertest-as-promised": "4.0.0",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0"
  },
  "jest": {
    "coverageReporters": [
      "json",
      "lcov",
      "html",
      "text"
    ]
  }
}
