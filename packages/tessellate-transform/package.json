{
  "name": "tessellate-transform",
  "version": "1.2.0",
  "description": "Tessellate JSX/JSON transformer.",
  "author": "Maximilian Fellner <maximilian.fellner@zalando.de>",
  "license": "MIT",
  "main": "dist/index.js",
  "publishConfig": {
    "tag": "next"
  },
  "bin": {
    "tessellate-transform": "./bin/tessellate-transform"
  },
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zalando-incubator/tessellate.git"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "dist": "babel -d dist lib",
    "start": "babel-node lib/cli.js",
    "lint": "eslint .",
    "test": "NODE_ENV=test jest --coverage",
    "flow-check": "flow check",
    "flow-typed-install": "rimraf flow-typed/npm/*_vx.x.x.js && flow-typed install -o",
    "prepublishOnly": "npm run clean && npm run dist"
  },
  "dependencies": {
    "acorn-jsx": "3.0.1",
    "js-yaml": "3.7.0",
    "mz": "2.6.0",
    "yargs": "6.4.0"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-plugin-syntax-flow": "6.18.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-latest-minimal": "1.1.2",
    "eslint": "3.11.1",
    "eslint-plugin-babel": "4.0.0",
    "eslint-plugin-flowtype": "2.28.2",
    "eslint-plugin-promise": "3.4.0",
    "eslint-plugin-react": "6.7.1",
    "flow-bin": "0.39.0",
    "flow-typed": "2.0.0",
    "jest": "18.1.0",
    "rimraf": "2.5.4"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "json",
      "lcov",
      "html",
      "text"
    ]
  }
}
