{
  "name": "tessellate-server",
  "version": "2.0.0-alpha.d484c6f9",
  "description": "Tessellate server runtime.",
  "author": "Maximilian Fellner <maximilian.fellner@zalando.de>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "tag": "next"
  },
  "engines": {
    "node": ">=7.6.0",
    "npm": ">=5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zalando-incubator/tessellate.git"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "tsc -p tsconfig.dist.json",
    "dist": "yarn run clean && yarn run build",
    "start": "ts-node example/server.ts",
    "lint": "tslint --type-check --project tsconfig.json",
    "test": "yarn run lint && TESSELLATE_LOG_LEVEL=warn jest --coverage",
    "docs": "typedoc --readme none --tsconfig tsconfig.json --out doc",
    "prepublishOnly": "yarn run dist"
  },
  "dependencies": {
    "change-case": "3.0.1",
    "express-winston": "2.4.0",
    "js-yaml": "3.10.0",
    "koa": "2.4.1",
    "koa-bodyparser": "4.2.0",
    "koa-compose": "4.0.0",
    "koa-router": "7.2.1",
    "mz": "2.7.0",
    "prom-client": "10.2.2",
    "prometheus-gc-stats": "0.5.0",
    "source-map-support": "0.5.0",
    "typeconf": "0.4.2",
    "winston": "2.4.0"
  },
  "devDependencies": {
    "@types/express": "4.0.39",
    "@types/jest": "21.1.5",
    "@types/js-yaml": "3.9.1",
    "@types/koa": "2.0.39",
    "@types/koa-bodyparser": "3.0.25",
    "@types/koa-compose": "3.2.2",
    "@types/koa-router": "7.0.23",
    "@types/mz": "0.0.32",
    "@types/node": "8.0.50",
    "@types/supertest": "2.0.3",
    "@types/winston": "2.3.7",
    "jest": "21.2.1",
    "rimraf": "2.6.2",
    "supertest": "3.0.0",
    "ts-jest": "21.2.1",
    "ts-node": "3.3.0",
    "tslint": "5.8.0",
    "typedoc": "0.9.0",
    "typescript": "2.6.1"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/../../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/test/.*\\.test.ts$",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ]
  }
}
