{
  "name": "tessellate-server",
  "version": "1.1.1",
  "description": "Tessellate server runtime.",
  "author": "Maximilian Fellner <maximilian.fellner@zalando.de>",
  "license": "MIT",
  "main": "dist/index.js",
  "publishConfig": {
    "tag": "next"
  },
  "engines": {
    "node": ">=v7.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zalando-incubator/tessellate.git"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "tsc -p .",
    "dist": "npm run clean && npm run build",
    "start": "ts-node example/server.ts",
    "lint": "tslint --type-check --project tsconfig.json",
    "test": "npm run lint && NODE_ENV=test jest --coverage",
    "flow-gen-files": "flow-copy-source -v src dist",
    "flow-typed-install": "rimraf flow-typed/npm && flow-typed install -o",
    "prepublishOnly": "npm run clean && npm run dist && npm run flow-gen-files",
    "upgrade-dependencies": "ncu -u"
  },
  "dependencies": {
    "change-case": "3.0.1",
    "express-winston": "2.4.0",
    "js-yaml": "3.8.4",
    "koa": "2.2.0",
    "koa-bodyparser": "4.2.0",
    "koa-compose": "4.0.0",
    "koa-router": "7.2.0",
    "mz": "2.6.0",
    "prom-client": "9.0.0",
    "prometheus-gc-stats": "0.3.4",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "@types/express": "4.0.35",
    "@types/jest": "19.2.3",
    "@types/js-yaml": "3.5.30",
    "@types/koa": "2.0.39",
    "@types/koa-bodyparser": "3.0.23",
    "@types/koa-compose": "3.2.2",
    "@types/koa-router": "7.0.22",
    "@types/mz": "0.0.31",
    "@types/supertest": "2.0.0",
    "@types/winston": "2.3.2",
    "jest": "20.0.1",
    "npm-check-updates": "2.11.1",
    "rimraf": "2.6.1",
    "ts-jest": "20.0.4",
    "ts-node": "3.0.4",
    "typescript": "2.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/test/.*\\.test.ts$",
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ]
  }
}
