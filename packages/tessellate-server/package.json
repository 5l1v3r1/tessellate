{
  "name": "tessellate-server",
  "version": "1.1.1",
  "description": "Tessellate server runtime.",
  "author": "Maximilian Fellner <maximilian.fellner@zalando.de>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "tag": "next"
  },
  "engines": {
    "node": ">=7.6.0",
    "npm": ">=5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zalando-incubator/tessellate.git"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "tsc -p .",
    "dist": "npm run clean && npm run build && npm run docs",
    "start": "ts-node example/server.ts",
    "lint": "tslint --type-check --project tsconfig.json && tslint test/**/.ts",
    "test": "npm run lint && TESSELLATE_LOG_LEVEL=error jest --coverage",
    "docs": "typedoc --readme none --tsconfig tsconfig.json --out doc",
    "prepublishOnly": "run dist",
    "upgrade-dependencies": "ncu -u"
  },
  "dependencies": {
    "change-case": "3.0.1",
    "express-winston": "2.4.0",
    "js-yaml": "3.10.0",
    "koa": "2.3.0",
    "koa-bodyparser": "4.2.0",
    "koa-compose": "4.0.0",
    "koa-router": "7.2.1",
    "mz": "2.6.0",
    "prom-client": "10.1.0",
    "prometheus-gc-stats": "0.5.0",
    "request": "2.81.0",
    "request-promise-native": "1.0.4",
    "source-map-support": "0.4.18",
    "typeconf": "0.4.1",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "@types/express": "4.0.37",
    "@types/jest": "20.0.8",
    "@types/js-yaml": "3.9.1",
    "@types/koa": "2.0.39",
    "@types/koa-bodyparser": "3.0.24",
    "@types/koa-compose": "3.2.2",
    "@types/koa-router": "7.0.23",
    "@types/mz": "0.0.31",
    "@types/request": "2.0.3",
    "@types/request-promise-native": "1.0.6",
    "@types/supertest": "2.0.3",
    "@types/winston": "2.3.5",
    "jest": "21.0.2",
    "npm-check-updates": "2.12.1",
    "rimraf": "2.6.1",
    "supertest": "3.0.0",
    "ts-jest": "21.0.0",
    "ts-node": "3.3.0",
    "tslint": "5.7.0",
    "typedoc": "0.8.0",
    "typescript": "2.5.2"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/test/.*\\.test.ts$",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ]
  }
}
