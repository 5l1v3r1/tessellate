{
  "name": "tessellate-bundler",
  "version": "0.1.4",
  "description": "Tessellate bundler service.",
  "author": "Maximilian Fellner <maximilian.fellner@zalando.de>",
  "license": "MIT",
  "main": "dist/server.js",
  "publishConfig": {
    "tag": "next"
  },
  "bin": {
    "tessellate-bundler": "./bin/tessellate-bundler"
  },
  "engines": {
    "node": ">=v7.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zalando-incubator/tessellate.git"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "dist": "babel -d dist src",
    "start": "DEBUG=tessellate-bundler:*,webpack-sandboxed:* babel-node src/server.js",
    "lint": "eslint src test",
    "test": "npm run lint && NODE_ENV=test jest --coverage",
    "flow-typed-install": "rimraf flow-typed/npm && flow-typed install -o",
    "prepublishOnly": "npm run clean && npm run dist",
    "upgrade-dependencies": "ncu -u"
  },
  "dependencies": {
    "babel-core": "6.23.1",
    "babel-loader": "6.3.2",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "change-case": "3.0.1",
    "css-loader": "0.26.2",
    "debug": "2.6.1",
    "extract-text-webpack-plugin": "2.0.0",
    "js-yaml": "3.8.1",
    "kcors": "2.2.0",
    "koa-static": "3.0.0",
    "mkdirp": "0.5.1",
    "mz": "2.6.0",
    "nconf": "0.8.4",
    "style-loader": "0.13.2",
    "tessellate-server": "1.1.0",
    "thenify": "3.2.1",
    "uuid": "3.0.1",
    "webpack": "2.2.1",
    "webpack-sandboxed": "0.2.4"
  },
  "peerDependencies": {
    "koa": "2.x.x",
    "koa-router-rx": "0.3.x",
    "rxjs": "5.x.x"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "eslint": "3.16.1",
    "flow-bin": "0.40.0",
    "flow-typed": "2.0.0",
    "jest": "19.0.2",
    "mosaic-component-example": "1.0.2",
    "npm-check-updates": "2.10.3",
    "rimraf": "2.6.1"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "json",
      "lcov",
      "html",
      "text"
    ]
  }
}
