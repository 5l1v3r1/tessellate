{
  "name": "tessellate-fragment",
  "version": "0.3.1",
  "description": "Tessellate fragment service.",
  "author": "Maximilian Fellner <maximilian.fellner@zalando.de>",
  "license": "MIT",
  "main": "dist/server.js",
  "publishConfig": {
    "tag": "next"
  },
  "bin": {
    "tessellate-fragment": "./bin/tessellate-fragment"
  },
  "engines": {
    "node": ">=v7.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zalando-incubator/tessellate.git"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "tsc -p tsconfig.dist.json",
    "dist": "yarn run clean && yarn run build",
    "start": "TESSELLATE_CONF=conf.yaml ts-node src/server.ts",
    "lint": "tslint --type-check --project tsconfig.json",
    "test": "yarn run lint && TESSELLATE_CONF=test/conf.yaml jest --coverage",
    "docs": "typedoc --readme none --tsconfig tsconfig.json --out doc",
    "prepublishOnly": "run dist"
  },
  "dependencies": {
    "lodash.merge": "4.6.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "request": "2.83.0",
    "request-promise-native": "1.0.5",
    "tessellate-server": "1.1.1"
  },
  "devDependencies": {
    "@types/lodash.merge": "4.6.3",
    "@types/node": "8.0.31",
    "@types/react": "16.0.7",
    "@types/react-dom": "15.5.5",
    "@types/request": "2.0.3",
    "@types/request-promise-native": "1.0.7",
    "@types/supertest": "2.0.3",
    "jest": "21.2.0",
    "rimraf": "2.6.2",
    "ts-jest": "21.0.1",
    "ts-node": "3.3.0",
    "tslint": "5.7.0",
    "typedoc": "0.8.0",
    "typescript": "2.5.3"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/../../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/test/.*\\.test.ts$",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ]
  }
}
