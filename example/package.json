    "react-transition-group": "2.2.1",
    "reactstrap": "5.0.0-alpha.3",
    "tessellate-bundler": "file:../packages/tessellate-bundler",
    "tessellate-fragment": "file:../packages/tessellate-fragment"
  }
}
